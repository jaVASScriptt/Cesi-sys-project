<Window x:Class="Prog.Sys___Workshop_1_v2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prog.Sys___Workshop_1_v2"
        DataContext="{Binding UpperModelView}"
        mc:Ignorable="d"
        Title="MainWindow" SnapsToDevicePixels="True" Height="470" Width="780">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50 "/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <Path Stretch="Uniform" Margin="0,10,0,0" VerticalAlignment="Top" Fill="#2a84f1" Height="25" Width="25"></Path>

        <ListBox x:Name="sidebar" Margin="0,50,0,50" BorderThickness="0">

            <local:Navbutton Margin="3" Padding="6"/>

        </ListBox>

        <Frame Grid.Column="1" x:Name="navframe" Background="#e9ecef"/>
    </Grid>
</Window>
